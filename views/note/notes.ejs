<%layout('layout/boilerplate')%>
<% notes.forEach(note => { %>
<div class="card" mb-3> 
  <div class="row">
    
  
  
  
    
    <div class="card mb-2">
      <div class="col-md-8">
      <div class="card-body">
          <h5 class="card-title"><%= note.title %></h5>
          <p class="text-muted">Posted On: <%= note.createdAt.toLocaleDateString() %></p>
          <a href="/notes/<%=note._id%>" class="btn btn-primary" >View Details</a>
        </div>
      </div>
    </div>
  </div> 
  </div>
  

  <%})%> 
  
</div>